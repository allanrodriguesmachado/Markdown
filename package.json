    "@react-native-community/netinfo": "^11.4.1",
    "axios": "^1.7.7",
    "correios-zebra-js": "^0.1.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "electron-i18n": "^1.3136.0",
    "flowbite-datepicker": "^1.3.0",
    "flowbite-react": "^0.10.1",
    "form-data": "^4.0.0",
    "ip": "^2.0.1",
    "moment": "^2.30.1",
    "node-wmi": "^0.0.5",
    "react-datepicker": "^7.5.0",
    "react-loader-spinner": "^6.1.6",
    "react-toastify": "^10.0.5"

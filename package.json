    "autoprefixer": "^10.4.20",
    "electron": "^32.1.0",
    "electron-builder": "^25.0.5",
    "electron-vite": "^2.3.0",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.36.1",
    "postcss": "^8.4.45",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.11",
    "vite": "^5.4.4"
